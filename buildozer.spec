[app]
# (fill with your application's name)
title = Supplement Tracker
# (fill with your application package name)
package.name = supplement_tracker
# (fill with your application package domain)
package.domain = org.example
# (fill with version information)
version = 0.1

# Application requirements
# You should specify the required Python packages for your app
# E.g., kivy,json
requirements = kivy,json

# Android settings
# These settings are used by buildozer to create your Android package
android.permissions = INTERNET
android.sdk_path = /path/to/your/android/sdk
android.ndk_path = /path/to/your/android/ndk
android.p4a = True

# (Optional) Include other necessary settings
